.wall.draggable
	position relative

	> .void
		position absolute
		left -200px
		top -200px
		background-color rgba(255, 0, 0, 0.5)

	> .grid
		background-color #ffa
		position absolute
		background url('/img/bg-grid.png')

	> .stickies
		list-style-type none
		overflow visible
		padding 0
		margin 0
		position relative
