.wall.stickie
  width 310px
  height 310px
  color black
  position absolute
  box-shadow inset 0px 0px 0px 2px rgba(0, 0, 0, 0.1)
  border-bottom 6px solid #303539
  padding 10px 20px 0 20px
  overflow hidden
  box-sizing border-box
  color #000
  font-size 34px
  font-weight 400
  line-height 41px

  > .coordinates
    position absolute
    bottom 0
    right 0
    height 20px
    width 70px
    font-size 10px
    color #fff
    background-color rgba(0, 0, 0, 0.25)
    text-align center
    line-height 20px
